@article{deshmukh2016Physioclimatic,
  title = {Physio-Climatic Controls on Vulnerability of Watersheds to Climate and Land Use Change across the {{U}}. {{S}}.: {{CONTROLS ON VULNERABILITY}}},
  shorttitle = {Physio-Climatic Controls on Vulnerability of Watersheds to Climate and Land Use Change across the {{U}}. {{S}}.},
