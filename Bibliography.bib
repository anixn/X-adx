@article{fang2013Impact,
  title = {The {{Impact}} of {{Potential Land Cover Misclassification}} on {{MODIS Leaf Area Index}} ({{LAI}}) {{Estimation}}: {{A Statistical Perspective}}},
  shorttitle = {The {{Impact}} of {{Potential Land Cover Misclassification}} on {{MODIS Leaf Area Index}} ({{LAI}}) {{Estimation}}},
