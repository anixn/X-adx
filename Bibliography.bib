@article{brouziyne2020Assessing,
  title = {Assessing {{Hydrological Vulnerability}} to {{Future Droughts}} in a {{Mediterranean Watershed}}: {{Combined Indices-Based}} and {{Distributed Modeling Approaches}}},
  shorttitle = {Assessing {{Hydrological Vulnerability}} to {{Future Droughts}} in a {{Mediterranean Watershed}}},
  author = {Brouziyne, Youssef and Abouabdillah, Aziz and Chehbouni, Abdelghani and Hanich, Lahoucine and Bergaoui, Karim and McDonnell, Rachael and Benaabidate, Lahcen},
  year = {2020},
